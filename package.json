{
  "name": "arena-fp-ts",
  "homepage": "https://github.com/arenadotio/arena-fp-ts",
  "version": "0.0.4",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arenadotio/arena-fp-ts.git"
  },
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "@sentry/node": "^7.56.0",
    "@sentry/serverless": "^7.56.0",
    "aws-lambda": "^1.0.7",
    "fp-ts": "^2.9.5",
    "fp-ts-contrib": "^0.1.29",
    "fp-ts-std": "^0.17.1",
    "io-ts": "^2.2.20",
    "io-ts-reporters": "^2.0.1",
    "logging-ts": "^0.3.4",
    "pino": "^8.14.1"
  },
  "dependencies": {},
  "devDependencies": {
    "@relmify/jest-fp-ts": "^2.1.1",
    "@sentry/integrations": "^7.56.0",
    "@sentry/node": "^7.56.0",
    "@sentry/serverless": "^7.56.0",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "aws-lambda": "^1.0.7",
    "cspell": "^5.5.2",
    "datadog-lambda-js": "^7.91.0",
    "docs-ts": "^0.6.10",
    "eslint": "^7.27.0",
    "fast-check": "^2.13.0",
    "fp-ts": "^2.9.5",
    "fp-ts-contrib": "^0.1.29",
    "fp-ts-std": "^0.17.1",
    "hot-shots": "^10.0.0",
    "io-ts": "^2.2.20",
    "io-ts-reporters": "^2.0.1",
    "jest": "^29.5.0",
    "jest-mock": "^29.5.0",
    "logging-ts": "^0.3.4",
    "markdown-magic": "^2.0.0",
    "mock-lambda-context": "^0.0.1",
    "pino": "^8.14.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "prettier-plugin-jsdoc": "^0.3.13",
    "ts-jest": "^29.1.0",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "build": "rm -rf ./dist && mkdir -p ./dist/esm ./dist/cjs/ && tsc -p ./tsconfig.build-esm.json && tsc -p ./tsconfig.build-cjs.json && tsc -p ./tsconfig.build-types.json",
    "format": "yarn run prettier --write .",
    "lint": "eslint ./src/ ./tests/ --ext ts",
    "test": "jest",
    "docs": "docs-ts",
    "spell": "cspell '**/*.*'",
    "md": "markdown",
    "prepublish": "yarn build"
  },
  "pre-commit": [
    "format",
    "lint",
    "docs",
    "spell"
  ]
}

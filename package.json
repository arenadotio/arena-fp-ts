{
  "name": "arena-fp-ts",
  "homepage": "https://github.com/arenadotio/arena-fp-ts",
  "version": "0.0.12",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arenadotio/arena-fp-ts.git"
  },
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "files": [
    "dist/"
  ],
  "peerDependencies": {
    "@sentry/node": "^7.56.0",
    "@sentry/serverless": "^7.56.0",
    "aws-lambda": "^1.0.7",
    "fp-ts": "^2.9.5",
    "io-ts": "^2.2.20",
    "io-ts-reporters": "^2.0.1",
    "pino": "^8.14.1"
  },
  "dependencies": {
    "@relmify/jest-fp-ts": "^2.1.1",
    "async-mutex": "^0.4.0",
    "fp-ts-contrib": "^0.1.29",
    "fp-ts-std": "^0.17.1",
    "jest-matcher-utils": "^29.6.1",
    "logging-ts": "^0.3.4",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5"
  },
  "devDependencies": {
    "@cspell/dict-en_us": "^4.3.4",
    "@fast-check/jest": "^1.7.0",
    "@sentry/integrations": "^7.56.0",
    "@sentry/node": "^7.56.0",
    "@sentry/serverless": "^7.56.0",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "aws-lambda": "^1.0.7",
    "cspell": "^6.31.1",
    "datadog-lambda-js": "^7.91.0",
    "docs-ts": "^0.8.0",
    "eslint": "^8.45.0",
    "fast-check": "^3.11.0",
    "fp-ts": "^2.9.5",
    "hot-shots": "^10.0.0",
    "io-ts": "^2.2.20",
    "io-ts-reporters": "^2.0.1",
    "jest": "^29.6.2",
    "jest-mock": "^29.5.0",
    "markdown-magic": "^2.0.0",
    "mock-lambda-context": "^0.0.1",
    "pino": "^8.14.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "prettier-plugin-jsdoc": "^0.3.13",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "build": "rm -rf ./dist && mkdir -p ./dist/esm ./dist/cjs/ && tsc -p ./tsconfig.build-esm.json && tsc -p ./tsconfig.build-cjs.json && tsc -p ./tsconfig.build-types.json",
    "format": "prettier --write .",
    "lint": "eslint ./src/ ./tests/ --ext ts",
    "test": "jest",
    "docs": "docs-ts",
    "spell": "cspell '**/*.*'",
    "md": "markdown",
    "prepublish": "npm run build"
  },
  "pre-commit": [
    "format",
    "lint",
    "docs",
    "spell"
  ]
}
